<p style="color: green"><%= notice %></p>

<strong>Zones</strong>
</br>
</br>
</br>
<div id="zones">
  <% @zones.each do |zone| %>
    <%= render zone %>
    <p>
      <%= link_to "Show this zone", zone %>
    </p>
  <% end %>
</div>
</br>

<h3>Import Zones</h3>
<%= form_tag import_zones_path, multipart: true do%>
  <%= file_field_tag :file %>
  <%= submit_tag "Upload" %>
<% end %>

<%= link_to "New zone", new_zone_path %>
